{
  "name": "@micra/theme-generator",
  "version": "0.0.1",
  "license": "MIT",
  "main": ".micra/index.cjs.js",
  "module": ".micra/index.js",
  "types": ".micra/index.d.ts",
  "files": [
    ".micra/**/*"
  ],
  "scripts": {
    "build": "yarn rimraf .micra && yarn rollup -c .config/build/rollup.config.js",
    "format": "prettier --write '**/*.{ts,tsx}' --config .config/linting/.prettierrc.js",
    "lint": "eslint '**/*.{ts,tsx}' --cache --fix --config .config/linting/.eslintrc.js --cache-location .config/linting/.eslintcache",
    "test": "jest --config .config/test/jest.config.js",
    "watch": "nodemon --config .config/dev/nodemon.json"
  },
  "devDependencies": {
    "@babel/core": "^7.10.4",
    "@babel/plugin-transform-react-jsx": "^7.10.4",
    "@babel/plugin-transform-typescript": "^7.10.4",
    "@babel/preset-env": "^7.10.4",
    "@types/jest": "^26.0.3",
    "@types/node": "^14.0.14",
    "@typescript-eslint/eslint-plugin": "^3.5.0",
    "@typescript-eslint/parser": "^3.5.0",
    "eslint": "^7.3.1",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-prettier": "^3.1.4",
    "jest": "^26.1.0",
    "nodemon": "^2.0.4",
    "prettier": "^2.0.4",
    "rimraf": "^3.0.2",
    "rollup": "^2.18.2",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-size-snapshot": "^0.12.0",
    "rollup-plugin-typescript2": "^0.27.1",
    "ts-jest": "^26.1.1",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.6"
  },
  "dependencies": {
    "prettier": "^2.0.5"
  }
}
