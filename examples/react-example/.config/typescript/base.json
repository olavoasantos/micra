{
  "compilerOptions": {
    "target": "es5",
    "module": "esnext",
    "lib": ["es6", "dom", "es2016", "es2017", "esnext"],
    "declaration": false,
    "removeComments": true,
    "importHelpers": true,
    "strict": true,
    "noImplicitAny": true,
    "alwaysStrict": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "skipLibCheck": true,
    "jsx": "react",
    "sourceMap": true,
    "inlineSources": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true
  }
}
